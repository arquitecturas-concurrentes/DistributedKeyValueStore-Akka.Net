<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="akka" type="Akka.Configuration.Hocon.AkkaConfigurationSection, Akka" />
  </configSections>
  <appSettings>
    <add key="maxKeyLength" value="20" />
    <add key="maxValueLength" value="20" />
    <add key="storagesAmount" value="5" />
    <add key="storage.address" value="akka.tcp://KvActorSystem@localhost:8095" />
  </appSettings>
  <akka>
    <hocon>
      <![CDATA[

          akka {
            loglevel = INFO

            actor {
              provider = "Akka.Remote.RemoteActorRefProvider, Akka.Remote"
              debug {
                receive = on
                autoreceive = on
                lifecycle = on
                event-stream = on
                unhandled = on
              }
            }

            remote {
              helios.tcp {
  	            port = 8095
  	            hostname = localhost
              }
            }
          }
        ]]>
    </hocon>
  </akka>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
</configuration>